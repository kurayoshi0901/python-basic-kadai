        "class Human:\n",
        "    def __init__(self, name, age):\n",
        "        self.name = name\n",
        "        self.age = age\n",
        "    def printinfo(self):\n",
        "        print(self.name)\n",
        "        print(self.age)\n",
        "\n",
        "person1 = Human(\"太郎\", 27)\n",
        "person2 = Human(\"次郎\", 33)\n",
        "\n",
        "person1.printinfo()\n",
        "person2.printinfo()\n"
      ]
